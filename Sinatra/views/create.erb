<h1> Nuevo taller </h1>
<form action="/create" method="post">
    <fieldset>
        <label for="name"> Nombre: </label> <br>
        <input type="text" name="name" id="name">
    </fieldset>
    <fieldset>
        <label for="description"> Descripcion: </label> <br>
        <textarea name="description" rows="10" cols="40" id="description"> </textarea>
    </fieldset>
    <fieldset>
        <input type="submit">
    </fieldset>
</form>